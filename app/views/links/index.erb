<h1>Welcome to your bookmark manager!</h1>
<h2><%= current_user.email %></h2>

<ul id='links'>
  <% @links.each do |link| %>
    <li>
      Title: <a href=<%= link.url %>> <%= link.title %> </a>
      Tags: <% link.tags.each do |a| %>
        <%= a.tag %>
      <% end %>
    </li>
  <% end %>
</ul>
